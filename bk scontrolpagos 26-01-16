-- Volcando estructura para vista scontrolpagos.comisionxcobrador
DROP VIEW IF EXISTS `comisionxcobrador`;
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `comisionxcobrador`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` VIEW `scontrolpagos`.`comisionxcobrador` AS SELECT soc_codigo_armado codigo_socio,
       tds_nombre tipo_socio,
       ps.per_nombre_completo nombre_socio,
       pag_año año_pago,
       elt(pag_mes, 'ENERO','FEBRERO','MARZO','ABRIL','MAYO','JUNIO','JULIO','AGOSTO','SEPTIEMBRE','NOVIEMBRE','DICIEMBRE') MES_PAGO,
       pag_numero_recibo numero_recibo,
       pag_monto monto_recibo,      
       pag_fecha_recibido fecha_recibo,
       pc.per_nombre_completo nombre_cobrador,
       cob_porcentaje_comision comision
  FROM soc_soc_socios,
       grl_per_personas ps,
       pag_pag_pagos,
       pag_cob_cobradores,
       grl_per_personas pc,
       soc_tds_tipos_de_socios
 WHERE     soc_codper = ps.per_codigo
       AND soc_codigo = pag_codsoc
       AND pag_codcob = cob_codigo
       AND cob_codper = pc.per_codigo
       AND tds_codigo = soc_codtds ;


-- Volcando estructura para vista scontrolpagos.controlpagos
DROP VIEW IF EXISTS `controlpagos`;
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `controlpagos`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` VIEW `scontrolpagos`.`controlpagos` AS select `soc_soc_socios`.`soc_codigo_armado` AS `codigo_socio`,
`grl_per_personas`.`per_nombre_completo` AS `nombre_socio`,
`grl_per_personas`.`per_DPI` AS `dpi_socio`,
`grl_per_personas`.`per_cedula` AS `cedula_socio`,
`soc_tds_tipos_de_socios`.`tds_nombre` AS `tipo_socio`,
elt(`pag_pag_pagos`.`pag_mes`,'ENERO','FEBRERO','MARZO','ABRIL','MAYO','JUNIO',
'JULIO','AGOSTO','SEPTIEMBRE','OCTUBRE','NOVIEMBRE','DICIEMBRE') AS `mes_pago`,
`pag_pag_pagos`.`pag_año` AS `anio_pago`,
`pag_pag_pagos`.`pag_numero_recibo` AS `numero_recibo` 
from (((`grl_per_personas` join `soc_soc_socios`) 
join `pag_pag_pagos`) join `soc_tds_tipos_de_socios`) 
where ((`grl_per_personas`.`per_codigo` = `soc_soc_socios`.`soc_codper`) 
and (`soc_tds_tipos_de_socios`.`tds_codigo` = `soc_soc_socios`.`soc_codtds`) 
and (`soc_soc_socios`.`soc_codigo` = `pag_pag_pagos`.`pag_codsoc`))
order by pag_mes, pag_año ;


-- Volcando estructura para vista scontrolpagos.sociosmorosos
DROP VIEW IF EXISTS `sociosmorosos`;
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `sociosmorosos`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` VIEW `scontrolpagos`.`sociosmorosos` AS (SELECT soc_codigo_armado AS codigo_socio,
       tds_codigo AS tipo_socio,
       per_nombre_completo AS nombre_socio,
       elt(pag_mes,
           'ENERO',
           'FEBRERO',
           'MARZO',
           'ABRIL',
           'MAYO',
           'JUNIO',
           'JULIO',
           'AGOSTO',
           'SEPTIEMBRE',
           'OCTUBRE',
           'NOVIEMBRE',
           'DICIEMBRE')
          AS `mes_pago`,
       pag_año AS anio_pago,
       pag_fecha AS pag_fecha,
       SOC_ESTADO
  FROM ultimopagosocio
       RIGHT JOIN soc_soc_socios ON Soc_codigo = pag_codsoc
       JOIN grl_per_personas ON per_codigo = soc_codper
       JOIN soc_tds_tipos_de_socios ON tds_codigo = soc_codtds
 WHERE soc_estado = 'A') ;


-- Volcando estructura para vista scontrolpagos.ultimopagosocio
DROP VIEW IF EXISTS `ultimopagosocio`;
-- Eliminando tabla temporal y crear estructura final de VIEW
DROP TABLE IF EXISTS `ultimopagosocio`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` VIEW `scontrolpagos`.`ultimopagosocio` AS SELECT `pag_pag_pagos`.`pag_codsoc` AS `pag_codsoc`,
       max(`pag_pag_pagos`.`pag_mes`) AS `pag_mes`,
       max(`pag_pag_pagos`.`pag_año`) AS `pag_año`,
       (  (  makedate(max(`pag_pag_pagos`.`pag_año`), 1)
           + INTERVAL max(`pag_pag_pagos`.`pag_mes`) MONTH)
        + INTERVAL -(1) DAY)
          AS `pag_fecha`
  FROM `pag_pag_pagos`
GROUP BY `pag_pag_pagos`.`pag_codsoc` ;
