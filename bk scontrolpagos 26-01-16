-- Volcando estructura de base de datos para scontrolpagos
DROP DATABASE IF EXISTS `scontrolpagos`;
CREATE DATABASE IF NOT EXISTS `scontrolpagos` /*!40100 DEFAULT CHARACTER SET latin1 */;
USE `scontrolpagos`;


-- Volcando estructura para procedimiento scontrolpagos.ActualizaPassword
DROP PROCEDURE IF EXISTS `ActualizaPassword`;
DELIMITER //
CREATE DEFINER=`root`@`localhost` PROCEDURE `ActualizaPassword`(IN _Usuario varchar(20), in _Pass varchar(20))
BEGIN
       set @sql=concat("SET PASSWORD FOR '", _Usuario, "'@'%' = PASSWORD('", _Pass, "');");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt;       
     END//
DELIMITER ;


-- Volcando estructura para vista scontrolpagos.comisionxcobrador
DROP VIEW IF EXISTS `comisionxcobrador`;
-- Creando tabla temporal para superar errores de dependencia de VIEW
CREATE TABLE `comisionxcobrador` (
	`codigo_socio` VARCHAR(20) NOT NULL COLLATE 'latin1_swedish_ci',
	`tipo_socio` VARCHAR(40) NOT NULL COLLATE 'latin1_swedish_ci',
	`nombre_socio` VARCHAR(120) NULL COLLATE 'latin1_swedish_ci',
	`año_pago` INT(4) NOT NULL,
	`MES_PAGO` VARCHAR(10) NULL COLLATE 'utf8_general_ci',
	`numero_recibo` INT(10) NOT NULL,
	`monto_recibo` DECIMAL(10,2) NOT NULL,
	`fecha_recibo` DATE NOT NULL,
	`nombre_cobrador` VARCHAR(120) NULL COLLATE 'latin1_swedish_ci',
	`comision` DECIMAL(10,2) NULL
) ENGINE=MyISAM;


-- Volcando estructura para vista scontrolpagos.controlpagos
DROP VIEW IF EXISTS `controlpagos`;
-- Creando tabla temporal para superar errores de dependencia de VIEW
CREATE TABLE `controlpagos` (
	`codigo_socio` VARCHAR(20) NOT NULL COLLATE 'latin1_swedish_ci',
	`nombre_socio` VARCHAR(120) NULL COLLATE 'latin1_swedish_ci',
	`dpi_socio` VARCHAR(30) NULL COLLATE 'latin1_swedish_ci',
	`cedula_socio` VARCHAR(30) NULL COLLATE 'latin1_swedish_ci',
	`tipo_socio` VARCHAR(40) NOT NULL COLLATE 'latin1_swedish_ci',
	`mes_pago` VARCHAR(10) NULL COLLATE 'utf8_general_ci',
	`anio_pago` INT(4) NOT NULL,
	`numero_recibo` INT(10) NOT NULL
) ENGINE=MyISAM;


-- Volcando estructura para procedimiento scontrolpagos.CrearUsuario
DROP PROCEDURE IF EXISTS `CrearUsuario`;
DELIMITER //
CREATE DEFINER=`root`@`localhost` PROCEDURE `CrearUsuario`(IN _Usuario varchar(20), in _Pass varchar(20))
BEGIN
       /*CREATE USER Usuario@'localhost' IDENTIFIED BY '123';*/
       set @sql=concat("create user '", _Usuario, "'@'%' identified by '", _Pass, "';");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt;
       
       set @sql=concat("GRANT DELETE ON scontrolpagos.* TO'", _Usuario, "'@'%' identified by '", _Pass, "';");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt;
                  
       set @sql=concat("GRANT INSERT ON scontrolpagos.* TO'", _Usuario, "'@'%' identified by '", _Pass, "';");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt;       
       
       set @sql=concat("GRANT SELECT ON scontrolpagos.* TO'", _Usuario, "'@'%' identified by '", _Pass, "';");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt;       
       
       set @sql=concat("GRANT UPDATE ON scontrolpagos.* TO'", _Usuario, "'@'%' identified by '", _Pass, "';");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt; 
       
       set @sql=concat("GRANT EXECUTE ON scontrolpagos.* TO'", _Usuario, "'@'%' identified by '", _Pass, "';");
       prepare stmt from @sql;
       execute stmt;
       deallocate prepare stmt; 
     END//
DELIMITER ;


-- Volcando estructura para tabla scontrolpagos.grl_per_personas
DROP TABLE IF EXISTS `grl_per_personas`;
CREATE TABLE IF NOT EXISTS `grl_per_personas` (
  `per_codigo` int(11) NOT NULL AUTO_INCREMENT,
  `per_primer_nombre` varchar(40) DEFAULT NULL,
  `per_segundo_nombre` varchar(40) DEFAULT NULL,
  `per_primer_apellido` varchar(40) DEFAULT NULL,
  `per_segundo_apellido` varchar(40) DEFAULT NULL,
  `per_apellido_casada` varchar(40) DEFAULT NULL,
  `per_nombre_completo` varchar(120) DEFAULT NULL,
  `per_DPI` varchar(30) DEFAULT NULL,
  `per_cedula` varchar(30) DEFAULT NULL,
  `per_departamento_extendida` varchar(2) DEFAULT NULL,
  `per_municipio_extendida` varchar(2) DEFAULT NULL,
  `per_direccion_cobro` varchar(120) DEFAULT NULL,
  `per_telefonos` varchar(10) DEFAULT NULL,
  `per_celulares` varchar(10) DEFAULT NULL,
  `per_correo_electronico` varchar(50) DEFAULT NULL,
  `per_fecha_nacimiento` date NOT NULL,
  `per_foto` mediumblob,
  `per_estado` enum('A','E') NOT NULL,
  `per_usuario_grabacion` varchar(20) DEFAULT NULL,
  `per_fecha_grabacion` datetime DEFAULT NULL,
  `per_usuario_modificacion` varchar(20) DEFAULT NULL,
  `per_fecha_modificacion` datetime DEFAULT NULL,
  PRIMARY KEY (`per_codigo`)
) ENGINE=InnoDB AUTO_INCREMENT=1002 DEFAULT CHARSET=latin1;

