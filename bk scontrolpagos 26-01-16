-- Volcando estructura para disparador scontrolpagos.trg_acc_creacion
DROP TRIGGER IF EXISTS `trg_acc_creacion`;
SET @OLDTMP_SQL_MODE=@@SQL_MODE, SQL_MODE='STRICT_TRANS_TABLES,NO_ENGINE_SUBSTITUTION';
DELIMITER //
CREATE TRIGGER `trg_acc_creacion` BEFORE INSERT ON `seg_acc_acciones` FOR EACH ROW if NEW.acc_fecha_grabacion  is null then
SET NEW.acc_fecha_grabacion = now(), new.acc_usuario_grabacion = user();
end if//
DELIMITER ;
SET SQL_MODE=@OLDTMP_SQL_MODE;


-- Volcando estructura para disparador scontrolpagos.trg_acc_modificacion
DROP TRIGGER IF EXISTS `trg_acc_modificacion`;
SET @OLDTMP_SQL_MODE=@@SQL_MODE, SQL_MODE='STRICT_TRANS_TABLES,NO_ENGINE_SUBSTITUTION';
DELIMITER //
